{
    "name": "cqh-python-import-helper",
    "displayName": "cqh-python-import-helper",
    "description": "",
    "version": "0.0.48",
    "publisher": "cqh",
    "engines": {
        "vscode": "^1.34.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/chen19901225/python-import-helper.git"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:python",
        "onCommand:cqh-python-import-helper.function_apply_self",
        "onCommand:cqh-python-import-helper.get_parent_args_dict",
        "onCommand:cqh-python-import-helper.get_parent_name",
        "onCommand:cqh-python-import-helper.delegate_to_parent",
        "onCommand:cqh-python-import-helper.get_parent_original_args"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "keybindings": [
            {
                "command": "cqh-python-import-helper.function_apply_self",
                "key": "alt+-",
                "when": "editorTextFocus && !editorHasSelection && editorLangId==python "
            },
            {
                "command": "cqh-python-import-helper.delegate_to_parent",
                "key": "alt+=",
                "when": "editorTextFocus && !editorHasSelection && editorLangId==python"
            },
            {
                "command": "cqh-python-import-helper.get_parent_name",
                "key": "alt+[",
                "when": "editorTextFocus && !editorHasSelection && editorLangId==python"
            },
            {
                "command": "cqh-python-import-helper.get_parent_args_dict",
                "key": "alt+]",
                "when": "editorTextFocus && !editorHasSelection && editorLangId==python"
            },
            {
                "command": "cqh-python-import-helper.get_parent_original_args",
                "key": "alt+o",
                "when": "editorTextFocus  && editorLangId==python"
            },
            {
                "command": "cqh-python-import-helper.dict_unpack",
                "key": "alt+i",
                "when": "editorTextFocus && !editorHasSelection && editorLangId==python"
            },
            {
                "command": "cqh-python-import-helper.dict_get_unpack",
                "key": "alt+o",
                "when": "editorTextFocus && !editorHasSelection && editorLangId==python"
            },
            {
                "command": "cqh-python-import-helper.dict_prepend",
                "key": "alt+u",
                "when": "editorTextFocus && !editorHasSelection && editorLangId==python"
            },
            {
                "command": "cqh-python-import-helper.get_left_pattern",
                "key": "alt+h",
                "when": "editorTextFocus && !editorHasSelection && editorLangId==python"
            },
            {
                "command": "cqh-python-import-helper.get_last_if_variable",
                "key": "alt+\\",
                "when": "editorTextFocus && !editorHasSelection && editorLangId==python"
            }
        ],
        "commands": [
            {
                "command": "cqh-python-import-helper.get_left_pattern",
                "title": "cqh-python-import-helper.get_left_pattern"
            },
            {
                "command": "cqh-python-import-helper.get_last_if_variable",
                "title": "cqh-python-import get last if variable"
            },
            {
                "command": "cqh-python-import-helper.import-upgrade",
                "title": "cqh python import import upgrade"
            },
            {
                "command": "cqh-python-import-helper.dict_prepend",
                "title": "cqh python import dict prepend"
            },
            {
                "command": "cqh-python-import-helper.function_apply_self",
                "title": "cqh python import def apply self"
            },
            {
                "command": "cqh-python-import-helper.get_parent_args_dict",
                "title": "cqh python import get caller args"
            },
            {
                "command": "cqh-python-import-helper.get_parent_name",
                "title": "cqh python import get caller function name"
            },
            {
                "command": "cqh-python-import-helper.delegate_to_parent",
                "title": "cqh python import delegate to caller"
            },
            {
                "command": "cqh-python-import-helper.dict_unpack",
                "title": "cqh python import dict unpack"
            },
            {
                "command": "cqh-python-import-helper.dict_get_unpack",
                "title": "cqh python import dict get unpack"
            },
            {
                "command": "cqh-python-import-helper.get_parent_original_args",
                "title": "cqh python get parent original args kwargs"
            }
        ],
        "menus": {
            "editor/context": [
                {
                    "when": "editorHasSelection && editorLangId == python",
                    "command": "cqh-python-import-helper.import-upgrade",
                    "group": "cqh-python-import"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}
