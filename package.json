{
    "name": "cqh-python-import-helper",
    "displayName": "cqh-python-import-helper",
    "description": "",
    "version": "0.0.12",
    "publisher": "cqh",
    "engines": {
        "vscode": "^1.34.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/chen19901225/python-import-helper.git"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:python"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "keybindings": [
			{
				"command": "cqh-python-import-help.select-current-line",
				"key": "Ctrl+L",
				"when": "editorTextFocus"
			}
        ],
        "commands": [
            {
                "command": "cqh-python-import-helper.import-upgrade",
                "title": "cqh python import import upgrade"
            },
            {
                "command": "cqh-python-import-help.select-current-line",
                "title": "cqh python import select current line"
            }
        ],
        "menus": {
            "editor/context": [
                {
                    "when": "editorHasSelection && editorLangId == python",
                    "command": "cqh-python-import-helper.import-upgrade",
                    "group": "cqh-python-import"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}
